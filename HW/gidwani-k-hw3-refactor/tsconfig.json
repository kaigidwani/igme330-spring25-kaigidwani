{
    "compilerOptions": {
      "target": "es5",
      "module": "es6",
      "moduleResolution": "node",
      "lib": ["dom", "es6"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist"
    },
    "include": ["src/**/*"]
  }
  